<h1>ユーザー一覧</h1>

<div class="users-index">
	<% @users.each do |user| %>
		<div class="users-index-content">
			<%= link_to user_path(user) do %>
				<%= attachment_image_tag user, :profile_image, size:'250x250', fallback: "no_image_icon.jpg" %>
				<h3><%= user.name %></h3>
			<% end %>
		</div>
	<% end %>
</div>
